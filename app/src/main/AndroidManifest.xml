<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="ar.edu.unlam.florenciamartin.tallerprogramacion">
    <!--
aca van los permisos que va a usar la aplicacion. Aca solo va el de
internet. Porqe el resto de los permisos son en tiempo de ejecucion. Ver en main activity
    -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".activity_item_lista"></activity>
        <activity android:name=".ListaProductosActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductoActivity" />
    </application>

</manifest>